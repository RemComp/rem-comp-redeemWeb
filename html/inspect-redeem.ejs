<!DOCTYPE html>

<html lang="en" class="light-style layout-menu-fixed layout-wide" dir="ltr" data-theme="theme-default"
  data-assets-path="../assets/">

<head>
  <meta charset="utf-8" />
  <meta name="viewport"
    content="width=device-width, initial-scale=1.0, user-scalable=no, minimum-scale=1.0, maximum-scale=1.0" />

  <title>RemCp. | Inspect Redeem</title>

  <meta name="description" content="" />

  <!-- Favicon -->
  <link rel="apple-touch-icon" sizes="180x180" href="../assets/img/favicon/apple-touch-icon.png">
  <link rel="icon" type="image/png" sizes="32x32" href="../assets/img/favicon/favicon-32x32.png">
  <link rel="icon" type="image/png" sizes="16x16" href="../assets/img/favicon/favicon-16x16.png">
  <link rel="manifest" href="../assets/img/favicon/site.webmanifest">

  <!-- Fonts -->
  <link rel="preconnect" href="https://fonts.googleapis.com" />
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
  <link
    href="https://fonts.googleapis.com/css2?family=Public+Sans:ital,wght@0,300;0,400;0,500;0,600;0,700;1,300;1,400;1,500;1,600;1,700&display=swap"
    rel="stylesheet" />

  <link rel="stylesheet" href="../assets/vendor/fonts/boxicons.css" />

  <!-- Core CSS -->
  <link rel="stylesheet" href="../assets/vendor/css/core.css" class="template-customizer-core-css" />
  <link rel="stylesheet" href="../assets/vendor/css/theme-default.css" class="template-customizer-theme-css" />
  <link rel="stylesheet" href="../assets/css/style.css" />

  <!-- Vendors CSS -->
  <link rel="stylesheet" href="../assets/vendor/libs/perfect-scrollbar/perfect-scrollbar.css" />

  <!-- Page CSS -->

  <!-- Helpers -->
  <script src="../assets/vendor/js/helpers.js"></script>
  <!--! Template customizer & Theme config files MUST be included after core stylesheets and helpers.js in the <head> section -->
  <!--? Config:  Mandatory theme config file contain global vars & default theme options, Set your preferred theme option in this file.  -->
  <script src="../assets/js/config.js"></script>
</head>

<body>
  <!-- Layout wrapper -->
  <div class="layout-wrapper layout-content-navbar">
    <div class="layout-container">
      <!-- Menu -->

      <%- include('page/sidebar', { pageActive, isAdmin }) %>
      <!-- / Menu -->

      <!-- Layout container -->
      <div class="layout-page"
        style="background-image: url('../assets/img/backgrounds/4907157.jpg'); background-size: cover; background-repeat: no-repeat; width: 100%; height: 100%;">
        <!-- Navbar -->

        <%- include('page/navbar', { moneyUser, profilePic, userName, roleUser }) %>

        <!-- / Navbar -->

        <!-- Content wrapper -->
        <div class="content-wrapper">
          
          <!-- Toast -->
          <div class="bs-toast toast toast-placement-ex m-3 top-0 end-0" role="alert" aria-live="assertive"
            aria-atomic="true" data-bs-delay="2000">
            <div class="toast-header">
              <i class="bx bx-bell me-2"></i>
              <div class="toast-title me-auto fw-medium"></div>
              <button type="button" class="btn-close" data-bs-dismiss="toast" aria-label="Close"></button>
            </div>
            <div class="toast-body"></div>
          </div>
          <!-- / Toast -->

          <!-- Content -->
          <div class="container-xl flex-grow-1 container-p-y">
            <!-- Layout Demo --> 
            <div class="col-xl">
              <div class="card mb-5">
                <div class="card-header d-flex align-items-center justify-content-between">
                  <h5 class="mb-0">Masukkan Kode Redeem</h5>
                </div>
                <div class="card-body">
                  <form class="inspect-redeem">
                    <div class="row mb-4">
                      <label class="col-sm-2 col-form-label" for="code-redeem-form">Redeem Code</label>
                      <div class="col-sm-10">
                        <input type="text" class="form-control" id="code-redeem-form" placeholder="Masukkan kode penukaran" />
                      </div>
                    </div>
                    <div class="d-grid gap-2 col-lg-6 mx-auto">
                      <button type="submit" class="btn btn-lg btn-dark inspect-redeem">SUBMIT</button>
                    </div>
                  </form>
                </div>
              </div>
            </div>

            <!-- table -->
            <div class="card mb-5">
              <h5 class="card-header">Item Reward</h5>
              <div class="table-responsive text-nowrap">
                <table class="table table-striped result-code-item">
                    <thead>
                      <tr>
                        <th>Item</th>
                        <th>Jumlah</th>
                        <th>Expired</th>
                      </tr>
                    </thead>
                    <tbody class="table-border-bottom-0">
                    </tbody>
                </table>
              </div>
            </div>

              <!-- Submit -->
              <div class="col-xl">
                <div class="card mb-5">
                  <div class="card-header d-flex align-items-center justify-content-between">
                    <h5 class="mb-0">Data Redeem</h5>
                  </div>
                  <div class="card-body">
                    <form class="result-code">
                      <div class="row mb-4">
                        <label class="col-sm-2 col-form-label" for="result-code-status">STATUS</label>
                        <div class="col-sm-10">
                          <div class="input-group input-group-merge">
                            <input type="text" class="form-control" id="result-code-status" value="" disabled />
                            <span class="input-group-text cursor-pointer" onclick="copyToClipboard('input#result-code-status', 'copy-status')">
                              <i class="bx bx-copy copy-status"></i>
                            </span>
                          </div>
                        </div>
                      </div>
                      <div class="row mb-4">
                        <label class="col-sm-2 col-form-label" for="result-code-id">ID</label>
                        <div class="col-sm-10">
                          <div class="input-group input-group-merge">
                            <input type="text" class="form-control" id="result-code-id" value="" disabled />
                            <span class="input-group-text cursor-pointer" onclick="copyToClipboard('input#result-code-id', 'copy-id')">
                              <i class="bx bx-copy copy-id"></i>
                            </span>
                          </div>
                        </div>
                      </div>
                      <div class="row mb-4">
                        <label class="col-sm-2 col-form-label" for="result-code-code">CODE</label>
                        <div class="col-sm-10">
                          <div class="input-group input-group-merge">
                            <input type="text" class="form-control" id="result-code-code" value="" disabled />
                            <span class="input-group-text cursor-pointer" onclick="copyToClipboard('input#result-code-code', 'copy-code')">
                              <i class="bx bx-copy copy-code"></i>
                            </span>
                          </div>
                        </div>
                      </div>
                      <div class="row mb-4">
                        <label class="col-sm-2 col-form-label" for="result-code-owner">OWNER</label>
                        <div class="col-sm-10">
                          <div class="input-group input-group-merge">
                            <input type="text" class="form-control" id="result-code-owner" value="" disabled />
                            <span class="input-group-text cursor-pointer" onclick="copyToClipboard('input#result-code-owner', 'copy-owner')">
                              <i class="bx bx-copy copy-owner"></i>
                            </span>
                          </div>
                        </div>
                      </div>
                      <div class="row mb-4">
                        <label class="col-sm-2 col-form-label" for="result-code-limit">LIMIT</label>
                        <div class="col-sm-10">
                          <div class="input-group input-group-merge">
                            <input type="text" class="form-control" id="result-code-limit" value="" disabled />
                            <span class="input-group-text cursor-pointer" onclick="copyToClipboard('input#result-code-limit', 'copy-limit')">
                              <i class="bx bx-copy copy-limit"></i>
                            </span>
                          </div>
                        </div>
                      </div>
                      <div class="row mb-4">
                        <label class="col-sm-2 col-form-label" for="result-code-created">WAKTU DIBUAT</label>
                        <div class="col-sm-10">
                          <div class="input-group input-group-merge">
                            <input type="text" class="form-control" id="result-code-created" value="" disabled />
                            <span class="input-group-text cursor-pointer" onclick="copyToClipboard('input#result-code-created', 'copy-created')">
                              <i class="bx bx-copy copy-created"></i>
                            </span>
                          </div>
                        </div>
                      </div>
                      <div class="row mb-4">
                        <label class="col-sm-2 col-form-label" for="result-code-expired">EXPIRED PADA</label>
                        <div class="col-sm-10">
                          <div class="input-group input-group-merge">
                            <input type="text" class="form-control" id="result-code-expired" value="" disabled />
                            <span class="input-group-text cursor-pointer" onclick="copyToClipboard('input#result-code-expired', 'copy-expired')">
                              <i class="bx bx-copy copy-expired"></i>
                            </span>
                          </div>
                        </div>
                      </div>
                      <div class="d-grid gap-2 mx-auto">
                        <button type="submit" class="btn btn-danger delete-redeem">HAPUS</button>
                      </div>
                    </form>
                  </div>
                </div>
              </div>
          </div>
          <!-- / Content -->

          <!-- Footer -->
          <%- include('page/footer') %>
          <!-- / Footer -->

          <div class="content-backdrop fade"></div>
        </div>
        <!-- Content wrapper -->
      </div>
      <!-- / Layout page -->
    </div>

    <!-- Overlay -->
    <div class="layout-overlay layout-menu-toggle"></div>
  </div>
  <!-- / Layout wrapper -->

  <!-- Core JS -->
  <!-- build:js assets/vendor/js/core.js -->

  <script src="../assets/vendor/libs/jquery/jquery.js"></script>
  <script src="../assets/vendor/libs/popper/popper.js"></script>
  <script src="../assets/vendor/js/bootstrap.js"></script>
  <script src="../assets/vendor/libs/perfect-scrollbar/perfect-scrollbar.js"></script>
  <script src="../assets/vendor/js/menu.js"></script>

  <!-- endbuild -->

  <!-- Vendors JS -->

  <!-- Main JS -->
  <script src="../assets/js/main.js"></script>

  <!-- Page JS -->
  <script src="../assets/js/toast.js"></script>
  <script src="../assets/js/constants/dataRedeem.js" defer></script>
  <script src="../assets/js/inspect-redeem.js"></script>
</body>

</html>
